CREATE TABLE RecipesEntity(
 id INTEGER NOT NULL PRIMARY KEY,
 title TEXT NOT NULL,
 videoThumbnail TEXT NOT NULL,
 videoDesc TEXT NOT NULL ,
 isVerified INTEGER  NOT NULL,
 channelSubs TEXT NOT NULL,
 likes TEXT NOT NULL ,
 channelName TEXT NOT NULL,
 channelImage TEXT NOT NULL,
 views TEXT NOT NULL,
 pubDate TEXT NOT NULL,
 duration TEXT NOT NULL
);

getVideoById:
SELECT * FROM RecipesEntity
WHERE id = :id;

getAllVideos:
SELECT * FROM RecipesEntity;

getVideoByTitle:
SELECT * FROM RecipesEntity
WHERE title = :title;

deleteVideoById:
DELETE FROM RecipesEntity
WHERE id = :id;

insertVideos:
INSERT OR REPLACE INTO RecipesEntity(id,title,videoThumbnail,videoDesc,isVerified,channelSubs,likes,channelName,channelImage,views,pubDate,duration)
VALUES (?,?,?,?,?,?,?,?,?,?,?,?);